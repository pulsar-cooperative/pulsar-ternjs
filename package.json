{
  "name": "pulsar-ternjs",
  "main": "./lib/atom-ternjs",
  "version": "0.20.1",
  "private": false,
  "description": "JavaScript code intelligence for Pulsar with tern. Adds support for ES5, ES6, ES7, ES8, Node.js and more. Extendable via plugins.",
  "activationHooks": [
    "language-javascript:grammar-used",
    "language-babel:grammar-used",
    "language-vue:grammar-used"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-ternjs:openConfig"
    ]
  },
  "repository": "https://github.com/pulsar-cooperative/pulsar-ternjs",
  "license": "MIT",
  "engines": {
    "atom": ">=1.13.0"
  },
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "event-kit": "^2.2.0",
    "glob": "^7.1.4",
    "loophole": "^1.1.0",
    "minimatch": "^3.0.4",
    "resolve-from": "^5.0.0",
    "tern": "^0.24.1",
    "underscore-plus": "^1.7.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-babel": "^5.3.0"
  },
  "package-deps": [
    "hyperclick"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    }
  },
  "keywords": [
    "javascript",
    "autocomplete-plus",
    "autocomplete",
    "node",
    "es6",
    "javascript-2015",
    "jquery",
    "tern"
  ]
}
